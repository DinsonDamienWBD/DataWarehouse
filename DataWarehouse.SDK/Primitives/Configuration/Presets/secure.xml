<?xml version="1.0" encoding="utf-8"?>
<DataWarehouseConfiguration>
  <PresetName>secure</PresetName>
  <Version>3.0.0</Version>
  <Security>
    <EncryptionEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></EncryptionEnabled>
    <AuthEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></AuthEnabled>
    <AuditEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></AuditEnabled>
    <TlsRequired><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></TlsRequired>
    <MfaEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></MfaEnabled>
    <RbacEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></RbacEnabled>
    <ZeroTrustMode><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></ZeroTrustMode>
    <FipsMode><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></FipsMode>
    <QuantumSafeMode><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></QuantumSafeMode>
    <HsmRequired><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></HsmRequired>
    <CertificatePinning><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></CertificatePinning>
    <TamperProofLogging><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></TamperProofLogging>
    <PasswordMinLength><Value>16</Value><AllowUserToOverride>true</AllowUserToOverride></PasswordMinLength>
    <SessionTimeoutMinutes><Value>15</Value><AllowUserToOverride>true</AllowUserToOverride></SessionTimeoutMinutes>
    <DefaultAuthScheme><Value>RBAC</Value><AllowUserToOverride>true</AllowUserToOverride></DefaultAuthScheme>
  </Security>
  <Storage>
    <EncryptAtRest><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></EncryptAtRest>
    <EnableCompression><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></EnableCompression>
    <EnableDeduplication><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></EnableDeduplication>
    <EnableCaching><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></EnableCaching>
    <EnableIndexing><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></EnableIndexing>
    <EnableVersioning><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></EnableVersioning>
    <CacheSizeBytes><Value>1073741824</Value><AllowUserToOverride>true</AllowUserToOverride></CacheSizeBytes>
    <MaxConnections><Value>100</Value><AllowUserToOverride>true</AllowUserToOverride></MaxConnections>
    <DefaultBackend><Value>FileSystem</Value><AllowUserToOverride>true</AllowUserToOverride></DefaultBackend>
    <DataDirectory><Value>./data</Value><AllowUserToOverride>true</AllowUserToOverride></DataDirectory>
  </Storage>
  <Network>
    <TlsEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></TlsEnabled>
    <Http2Enabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></Http2Enabled>
    <Http3Enabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></Http3Enabled>
    <CompressionEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></CompressionEnabled>
    <ListenPort><Value>8080</Value><AllowUserToOverride>true</AllowUserToOverride></ListenPort>
    <MaxConnectionsPerEndpoint><Value>1000</Value><AllowUserToOverride>true</AllowUserToOverride></MaxConnectionsPerEndpoint>
    <TimeoutSeconds><Value>30</Value><AllowUserToOverride>true</AllowUserToOverride></TimeoutSeconds>
    <BindAddress><Value>0.0.0.0</Value><AllowUserToOverride>true</AllowUserToOverride></BindAddress>
  </Network>
  <Replication>
    <Enabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></Enabled>
    <MultiMasterEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></MultiMasterEnabled>
    <CrdtEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></CrdtEnabled>
    <ReplicationFactor><Value>3</Value><AllowUserToOverride>true</AllowUserToOverride></ReplicationFactor>
    <ConsistencyLevel><Value>1</Value><AllowUserToOverride>true</AllowUserToOverride></ConsistencyLevel>
    <SyncIntervalSeconds><Value>60</Value><AllowUserToOverride>true</AllowUserToOverride></SyncIntervalSeconds>
    <ConflictResolutionStrategy><Value>LastWriteWins</Value><AllowUserToOverride>true</AllowUserToOverride></ConflictResolutionStrategy>
  </Replication>
  <Encryption>
    <Enabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></Enabled>
    <EncryptInTransit><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></EncryptInTransit>
    <EncryptAtRest><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></EncryptAtRest>
    <EnvelopeEncryption><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></EnvelopeEncryption>
    <KeyRotationEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></KeyRotationEnabled>
    <KeyRotationDays><Value>30</Value><AllowUserToOverride>true</AllowUserToOverride></KeyRotationDays>
    <DefaultAlgorithm><Value>AES256-GCM</Value><AllowUserToOverride>true</AllowUserToOverride></DefaultAlgorithm>
    <KeyStoreBackend><Value>FileSystem</Value><AllowUserToOverride>true</AllowUserToOverride></KeyStoreBackend>
  </Encryption>
  <Compression>
    <Enabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></Enabled>
    <AutoSelect><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></AutoSelect>
    <CompressionLevel><Value>6</Value><AllowUserToOverride>true</AllowUserToOverride></CompressionLevel>
    <DefaultAlgorithm><Value>zstd</Value><AllowUserToOverride>true</AllowUserToOverride></DefaultAlgorithm>
  </Compression>
  <Observability>
    <MetricsEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></MetricsEnabled>
    <TracingEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></TracingEnabled>
    <LoggingEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></LoggingEnabled>
    <HealthCheckEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></HealthCheckEnabled>
    <AnomalyDetectionEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></AnomalyDetectionEnabled>
    <LogLevel><Value>Information</Value><AllowUserToOverride>true</AllowUserToOverride></LogLevel>
    <MetricsIntervalSeconds><Value>60</Value><AllowUserToOverride>true</AllowUserToOverride></MetricsIntervalSeconds>
  </Observability>
  <Compute>
    <GpuEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></GpuEnabled>
    <ParallelProcessingEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></ParallelProcessingEnabled>
    <MaxWorkerThreads><Value>0</Value><AllowUserToOverride>true</AllowUserToOverride></MaxWorkerThreads>
    <MaxIoThreads><Value>0</Value><AllowUserToOverride>true</AllowUserToOverride></MaxIoThreads>
    <SchedulingStrategy><Value>FairShare</Value><AllowUserToOverride>true</AllowUserToOverride></SchedulingStrategy>
  </Compute>
  <Resilience>
    <CircuitBreakerEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></CircuitBreakerEnabled>
    <RetryEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></RetryEnabled>
    <BulkheadEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></BulkheadEnabled>
    <SelfHealingEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></SelfHealingEnabled>
    <MaxRetries><Value>3</Value><AllowUserToOverride>true</AllowUserToOverride></MaxRetries>
    <CircuitBreakerThreshold><Value>5</Value><AllowUserToOverride>true</AllowUserToOverride></CircuitBreakerThreshold>
    <TimeoutSeconds><Value>30</Value><AllowUserToOverride>true</AllowUserToOverride></TimeoutSeconds>
  </Resilience>
  <Deployment>
    <AirGapMode><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></AirGapMode>
    <MultiCloudEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></MultiCloudEnabled>
    <EdgeEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></EdgeEnabled>
    <EmbeddedMode><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></EmbeddedMode>
    <EnvironmentType><Value>Production</Value><AllowUserToOverride>true</AllowUserToOverride></EnvironmentType>
  </Deployment>
  <DataManagement>
    <CatalogEnabled><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></CatalogEnabled>
    <GovernanceEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></GovernanceEnabled>
    <QualityEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></QualityEnabled>
    <LineageEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></LineageEnabled>
    <MultiTenancyEnabled><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></MultiTenancyEnabled>
  </DataManagement>
  <MessageBus>
    <PersistentMessages><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></PersistentMessages>
    <OrderedDelivery><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></OrderedDelivery>
    <MaxQueueSize><Value>10000</Value><AllowUserToOverride>true</AllowUserToOverride></MaxQueueSize>
    <DeliveryTimeoutSeconds><Value>30</Value><AllowUserToOverride>true</AllowUserToOverride></DeliveryTimeoutSeconds>
  </MessageBus>
  <Plugins>
    <AutoLoadPlugins><Value>true</Value><AllowUserToOverride>true</AllowUserToOverride></AutoLoadPlugins>
    <AllowHotReload><Value>false</Value><AllowUserToOverride>true</AllowUserToOverride></AllowHotReload>
    <MaxPlugins><Value>100</Value><AllowUserToOverride>true</AllowUserToOverride></MaxPlugins>
    <PluginDirectory><Value>./plugins</Value><AllowUserToOverride>true</AllowUserToOverride></PluginDirectory>
  </Plugins>
</DataWarehouseConfiguration>
