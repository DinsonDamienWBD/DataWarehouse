<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Ultimate Data Format Plugin - Comprehensive data format parsing, serialization, and conversion with 10+ strategies including text formats (JSON, XML, CSV, YAML, TOML), binary formats (Protobuf, MessagePack), and schema formats (Avro, Thrift)</Description>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\DataWarehouse.SDK\DataWarehouse.SDK.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="YamlDotNet" Version="16.3.0" />
    <PackageReference Include="MessagePack" Version="3.1.4" />
    <PackageReference Include="Parquet.Net" Version="5.1.0" />
    <PackageReference Include="Apache.Arrow" Version="21.0.0" />
  </ItemGroup>
</Project>
