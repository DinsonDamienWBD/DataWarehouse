<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>DataWarehouse.Plugins.UltimateStorage</RootNamespace>
    <Description>Ultimate Storage Plugin - Comprehensive storage backend strategies including local, cloud (AWS S3, Azure Blob, GCS), network (NFS, SMB), and specialized storage (IPFS, Arweave)</Description>
    <Authors>DataWarehouse</Authors>
    <Company>DataWarehouse</Company>
    <Product>DataWarehouse Ultimate Storage</Product>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\DataWarehouse.SDK\DataWarehouse.SDK.csproj" />
  </ItemGroup>

  <!-- Cloud Storage Providers -->
  <ItemGroup>
    <!-- AWS S3 -->
    <PackageReference Include="AWSSDK.S3" Version="4.0.18.4" />

    <!-- Azure Blob Storage -->
    <PackageReference Include="Azure.Storage.Blobs" Version="12.27.0" />
    <PackageReference Include="Azure.Identity" Version="1.17.1" />

    <!-- Google Cloud Storage -->
    <PackageReference Include="Google.Cloud.Storage.V1" Version="4.14.0" />

    <!-- MinIO (S3-compatible) -->
    <PackageReference Include="Minio" Version="7.0.0" />

    <!-- Alibaba Cloud OSS -->
    <PackageReference Include="Aliyun.OSS.SDK.NetCore" Version="2.14.1" />

    <!-- Tencent Cloud COS -->
    <PackageReference Include="Tencent.QCloud.Cos.Sdk" Version="5.4.51" />

    <!-- Oracle Cloud Infrastructure Object Storage -->
    <PackageReference Include="OCI.DotNetSDK.Objectstorage" Version="127.1.0" />
    <PackageReference Include="OCI.DotNetSDK.Common" Version="127.1.0" />
  </ItemGroup>

  <!-- Database Storage -->
  <ItemGroup>
    <!-- MongoDB -->
    <PackageReference Include="MongoDB.Driver" Version="3.6.0" />
    <PackageReference Include="MongoDB.Driver.GridFS" Version="2.30.0" />

    <!-- PostgreSQL (large objects) -->
    <PackageReference Include="Npgsql" Version="10.0.1" />
  </ItemGroup>

  <!-- Distributed & Decentralized Storage -->
  <ItemGroup>
    <!-- IPFS HTTP Client -->
    <PackageReference Include="Ipfs.Http.Client" Version="0.33.0" />

    <!-- BitTorrent Client -->
    <PackageReference Include="MonoTorrent" Version="3.0.2" />
  </ItemGroup>

  <!-- Network Storage -->
  <ItemGroup>
    <!-- SMB/CIFS support -->
    <PackageReference Include="SMBLibrary" Version="1.5.5.1" />

    <!-- FTP support -->
    <PackageReference Include="FluentFTP" Version="53.0.2" />

    <!-- SFTP/SCP support -->
    <PackageReference Include="SSH.NET" Version="2025.1.0" />
  </ItemGroup>

  <!-- Object Storage & Key-Value Stores -->
  <ItemGroup>
    <!-- Redis -->
    <PackageReference Include="StackExchange.Redis" Version="2.10.14" />

    <!-- Memcached -->
    <PackageReference Include="EnyimMemcachedCore" Version="3.4.5" />

    <!-- FoundationDB -->
    <PackageReference Include="FoundationDB.Client" Version="7.3.2" />
  </ItemGroup>

  <!-- Remote Storage Protocols -->
  <ItemGroup>
    <!-- gRPC Client -->
    <PackageReference Include="Grpc.Net.Client" Version="2.76.0" />
    <PackageReference Include="Google.Protobuf" Version="3.33.5" />
  </ItemGroup>

  <!-- Data Connectors (Phase E) -->
  <ItemGroup>
    <!-- ODBC/Database Connectors -->
    <PackageReference Include="System.Data.Odbc" Version="10.0.3" />
    <PackageReference Include="System.Data.SqlClient" Version="4.9.0" />
    <PackageReference Include="MySql.Data" Version="9.6.0" />
    <PackageReference Include="Oracle.ManagedDataAccess.Core" Version="23.26.100" />

    <!-- Kafka & Streaming -->
    <PackageReference Include="Confluent.Kafka" Version="2.13.0" />

    <!-- Additional Dependencies -->
    <PackageReference Include="System.Linq.Async" Version="7.0.0" />
  </ItemGroup>

</Project>
