<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>DataWarehouse.Plugins.EmbeddedDatabaseStorage</RootNamespace>
    <AssemblyName>DataWarehouse.Plugins.EmbeddedDatabaseStorage</AssemblyName>
    <Description>DataWarehouse Embedded Database Storage Plugin - File-based embedded databases (SQLite, LiteDB)</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LiteDB" Version="5.0.21" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="10.0.2" />
    <PackageReference Include="RocksDb" Version="10.4.2.63147" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\DataWarehouse.SDK\DataWarehouse.SDK.csproj" />
  </ItemGroup>

</Project>
