<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <Description>SigNoz observability plugin with OTLP (OpenTelemetry Protocol) support for metrics, logs, and traces</Description>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <!-- CS1591: XML doc warnings suppressed - internal plugin strategies have self-documenting names -->
    <NoWarn>$(NoWarn);CS1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\DataWarehouse.SDK\DataWarehouse.SDK.csproj" />
  </ItemGroup>

</Project>
