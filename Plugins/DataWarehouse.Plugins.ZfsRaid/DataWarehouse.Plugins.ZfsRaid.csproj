<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>DataWarehouse.Plugins.ZfsRaid</RootNamespace>
    <Description>Production-ready ZFS RAID plugin for DataWarehouse supporting RAID-Z1/Z2/Z3 with variable stripe width, end-to-end checksums, self-healing, copy-on-write semantics, scrubbing, resilvering, deduplication, compression, snapshots, and dynamic block sizing</Description>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\DataWarehouse.SDK\DataWarehouse.SDK.csproj" />
  </ItemGroup>

</Project>
