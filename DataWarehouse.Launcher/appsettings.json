{
  "Kernel": {
    "Mode": "Workstation",
    "KernelId": null,
    "PluginPath": "./plugins"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "Path": "./logs"
  },
  "Storage": {
    "DefaultProvider": "local",
    "BasePath": "./data",
    "CacheEnabled": true,
    "CacheSize": "1GB"
  },
  "Security": {
    "EncryptionEnabled": false,
    "Algorithm": "AES-256-GCM",
    "KeyStorePath": "./keys"
  },
  "Pipeline": {
    "CompressionEnabled": true,
    "CompressionLevel": "Optimal",
    "MaxConcurrency": 0
  },
  "Health": {
    "CheckInterval": 30,
    "EnableMetrics": true,
    "MetricsPort": 9090
  }
}
